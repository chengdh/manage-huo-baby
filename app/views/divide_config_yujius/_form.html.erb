<%= semantic_form_for resource ,:html => {:id => "#{resource_class.name.underscore}_form"} do |f|%>
  <%=f.error_messages %>
  <%= f.inputs do %>
    <%= f.input :org_id,:as => :select, :collection => orgs_for_select %>

    <%= f.input :rate_from,:as => :string,:input_html => { :size => 10 }  %>
    <%= f.input :rate_to,:as => :string,:input_html => { :size => 10 }  %>
    <%= f.input :rate_inner_transit_from,:as => :string,:input_html => { :size => 10 }  %>
    <%= f.input :rate_inner_transit_to,:as => :string,:input_html => { :size => 10 }  %>
    <%= f.input :rate_outter_transit_from,:as => :string,:input_html => { :size => 10 }  %>
    <%= f.input :is_active %>
  <% end %>
<% end%>
