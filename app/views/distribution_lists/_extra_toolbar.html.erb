<%if params[:action] == "show"%>
  <li>
    <%= link_to image_tag("document-excel-table.png",:alt => "导出为EXCEL"), export_excel_distribution_list_path(resource,:format => :js),:title => "导出为EXCEL",:class => "tipsy"  %>
  </li>
  <%if can? :show_print_th_bill,resource%>
    <li>
      <%= link_to image_tag("barcode-icon.png",:alt => "批量打印提货单",:size => "16x16"), show_print_th_bill_distribution_list_path(resource),:title => "批量打印提货单",:class => "tipsy"  %>
    </li>
  <%end%>
<%end%>
<%if params[:action] == "show_print_th_bill" %>
  <li>
    <%= link_to image_tag("printer.png",:alt => "仅打印提货单"), "#",
      :title => "仅打印提货单",
      "data-print-template" => "#{render 'deliver_infos/th_bill' }",
      :class => "tipsy btn_print_th_bills_in_distribution_list" %>
  </li>
  <%if can? :print_th_bill_add_service_fee,resource%>
    <li>
      <%= link_to image_tag("printer-share.png",:alt => "仅打印提货单-添加15%服务费"), "#",
        :title => "仅打印提货单-添加15%服务费",
        "data-print-template" => "#{render 'deliver_infos/th_bill' }",
        :class => "tipsy btn_print_th_bills_in_distribution_list_add_service_fee" %>
      </li>
    <%end%>

<%end%>



