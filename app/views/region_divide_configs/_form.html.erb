<%= semantic_form_for resource, :html => {:id => "region_divide_config_form"} do |f|%>
  <%=f.error_messages %>
  <%= f.inputs do %>
    <%= f.input :from_region_id,:as => :select,:collection => regions_for_select,:include_blank => false%>
    <%= f.input :to_region_id,:as => :select,:collection => regions_for_select,:include_blank => false %>
    <%= f.input :from_rate %>
    <%= f.input :to_rate %>
    <%= f.input :summary_rate %>
    <%= f.input :note,:as => :string,:input_html => {:size => 60} %>
  <% end %>
<% end%>
